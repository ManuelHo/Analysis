								<sc:state id="Print_f"/>
							</sc:state>
							<sc:transition target="Pay" cond="$isInState('Archive_f') and $isInState('Print_f')"/>
						</sc:parallel>
						<sc:state id="Pay">
							<sc:transition event="finishedPayment" target="End2"/>
						</sc:state>
						<sc:state id="End2">
						</sc:state>
					  </sc:scxml>
				</elements>
			</childLevel>
		</topLevel>
		<concretizations>
		</concretizations>
	</mba>
  </concretizations>
</mba>